ccom:BeliefReification
      rdf:type prv:PropertyReification ;
      rdfs:label "Belief Reification"@en ;
      rdfs:comment "This is the property reification definition for shortcut relations with the property cco:belief as predicate."@en ;
      rdfs:isDefinedBy ccom: ;
      prv:object_property cco:topic ;
      prv:reification_class
              cco:CognitiveCharacteristic ;
      prv:shortcut cco:belief ;
      prv:shortcut_property
              cco:characteristic ;
      prv:subject_property
              cco:agent ;
      status:term_status "stable"@en .

