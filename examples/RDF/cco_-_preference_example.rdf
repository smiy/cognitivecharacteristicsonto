        <wo:weight>
          <wo:Weight>
            <wo:scale rdf:resource="http://example.org/AScale"/>
            <wo:weight_scale rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5.0</wo:weight_scale>
          </wo:Weight>
        </wo:weight>
      </cco:CognitiveCharacteristic>
